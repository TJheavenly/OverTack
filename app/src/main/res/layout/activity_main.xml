<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="overtack.com.overtack.MainActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hello TJ!" />

    <ToggleButton
        android:id="@+id/toggleButton1-1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="86dp"
        android:text="ToggleButton" />

    <ToggleButton
        android:id="@+id/toggleButton1-2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/toggleButton1-1"
        android:layout_toEndOf="@+id/toggleButton1-1"
        android:text="ToggleButton" />

    <ToggleButton
        android:id="@+id/toggleButton1-3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/toggleButton1-1"
        android:layout_toEndOf="@+id/toggleButton1-2"
        android:text="ToggleButton" />

    <ToggleButton
        android:id="@+id/toggleButton1-4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/toggleButton1-1"
        android:layout_toEndOf="@+id/toggleButton1-3"
        android:text="ToggleButton" />

    <ToggleButton
        android:id="@+id/toggleButton2-1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/toggleButton1-1"
        android:text="ToggleButton" />

    <ToggleButton
        android:id="@+id/toggleButton2-2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/toggleButton1-1"
        android:layout_toEndOf="@+id/toggleButton1-1"
        android:text="ToggleButton" />

    <ToggleButton
        android:id="@+id/toggleButton2-3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/toggleButton1-1"
        android:layout_toEndOf="@+id/toggleButton1-2"
        android:text="ToggleButton" />

    <ToggleButton
        android:id="@+id/toggleButton2-4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/toggleButton1-1"
        android:layout_toEndOf="@+id/toggleButton1-3"
        android:text="ToggleButton" />

    <ToggleButton
        android:id="@+id/toggleButton3-1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/toggleButton2-1"
        android:text="ToggleButton" />

    <ToggleButton
        android:id="@+id/toggleButton3-2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/toggleButton2-1"
        android:layout_toEndOf="@+id/toggleButton1-1"
        android:text="ToggleButton" />

    <ToggleButton
        android:id="@+id/toggleButton3-3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/toggleButton2-1"
        android:layout_toEndOf="@+id/toggleButton1-2"
        android:text="ToggleButton" />

    <ToggleButton
        android:id="@+id/toggleButton3-4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/toggleButton2-1"
        android:layout_toEndOf="@+id/toggleButton1-3"
        android:text="ToggleButton" />

    <ToggleButton
        android:id="@+id/toggleButton4-1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/toggleButton3-1"
        android:text="ToggleButton" />

    <ToggleButton
        android:id="@+id/toggleButton4-2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/toggleButton3-1"
        android:layout_toEndOf="@+id/toggleButton1-1"
        android:text="ToggleButton" />

    <ToggleButton
        android:id="@+id/toggleButton4-3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/toggleButton3-1"
        android:layout_toEndOf="@+id/toggleButton1-2"
        android:text="ToggleButton" />

    <ToggleButton
        android:id="@+id/toggleButton4-4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/toggleButton3-1"
        android:layout_toEndOf="@+id/toggleButton1-3"
        android:text="ToggleButton" />


</RelativeLayout>
